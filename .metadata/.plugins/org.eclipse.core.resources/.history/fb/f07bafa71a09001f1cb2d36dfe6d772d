package ncs.test6;

public class Provider extends Thread {
	private Data data;

	public Provider(Data data) {
		this.data = data;
	}

	@Override
	public void run() {
		for (int i = 0; i < 10; i++) {
			try {
				int a = (int) (Math.random() * 100) + 1;
				data.setValue(a);
				System.out.println("값이 입력되었습니다.");
				System.out.println("put value : "+a);
				sleep(1000);
			} catch (InterruptedException e) {
				e.printStackTrace();
			}
		}
	}
}
