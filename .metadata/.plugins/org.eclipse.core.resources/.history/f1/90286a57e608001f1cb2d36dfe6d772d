package ncs.test3;

import java.util.Scanner;

public class CalendarTest {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		System.out.println("날짜를 입력해주세요.");
		System.out.print("년 : ");
		System.out.print("월 : ");
		System.out.print("일 : ");
		
		
		
		
		sc.close();
	}
}
