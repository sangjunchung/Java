package com.calc;

import java.util.Scanner;

public class Calculator {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		System.out.print("첫번째 한 자리 정수(1~9) : ");
		int num1 = sc.nextInt();
		System.out.print("두번째 한 자리 정수(1~9) : ");
		int num2 = sc.nextInt();
		
		
		sc.close();
	}
}
